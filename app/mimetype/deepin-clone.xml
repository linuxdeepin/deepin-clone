<?xml version="1.0" encoding="UTF-8"?>
<mime-info xmlns="http://www.freedesktop.org/standards/shared-mime-info">
   <mime-type type="application/x-deepinclone-dim">
      <comment>Deepin Virtual Disk Image File</comment>
      <comment xml:lang="zh_CN">深度虚拟镜像文件</comment>
      <glob pattern="*.dim"/>
      <icon name="application-x-deepinclone-dim"/>
      <magic priority="80">
        <match value="0xdd0100dd" type="big32" offset="0" mask="0xffff00ff"/>
      </magic>
   </mime-type> 
</mime-info>
